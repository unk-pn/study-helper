{
  "prisma.pinToPrisma6": true,
  "workbench.editor.customLabels.patterns": {
    "**/app/**/{page,layout,index}.{ts,tsx}": "(${dirname})/${filename}.${extname}",
    "**/index.{ts,tsx}": "${dirname}/index.${extname}",
    "**/route.{ts}": "${dirname}/route.${extname}",
    "**/page.module.{css,scss,sass}": "${dirname}/page.${extname}"
  },
  "WillLuke.nextjs.addTypesOnSave": true,
  "WillLuke.nextjs.hasPrompted": true,
  "typescript.tsserver.maxTsServerMemory": 4096,
  "search.exclude": {
    "**/node_modules/**": true,
    "**/.next/**": true
  },
  "cSpell.words": ["Dont", "languagedetector", "persistor"],
  "editor.minimap.enabled": false
}
